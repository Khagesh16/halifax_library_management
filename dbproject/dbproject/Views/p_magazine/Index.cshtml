@model IEnumerable<dbproject.p_magazine>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.magazine_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.magazine_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.magazine_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.magazine_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.magazine_id }) |
            @Html.ActionLink("Details", "Details", new { id = item.magazine_id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.magazine_id })
        </td>
    </tr>
}

</table>
