@model IEnumerable<dbproject.p_author_articles>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.p_articles.title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.p_author.lname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.p_author.fname)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.p_articles.title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.p_author.lname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.p_author.fname)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.C_id }) |
            @Html.ActionLink("Details", "Details", new { id = item.C_id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.C_id })
        </td>
    </tr>
}

</table>
