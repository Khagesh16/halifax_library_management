@model IEnumerable<dbproject.p_publication>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.publication_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.volume_number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.published_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.p_magazine.magazine_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.publication_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.volume_number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.published_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.p_magazine.magazine_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.publication_id }) |
            @Html.ActionLink("Details", "Details", new { id = item.publication_id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.publication_id })
        </td>
    </tr>
}

</table>
